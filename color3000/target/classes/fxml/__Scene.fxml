<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" fx:controller="it.univaq.mwt.javafx.business.controller.CalcolatriceStandardController">
<top>
<Pane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
<children>
<Label alignment="TOP_LEFT" text="Calcolatrice">
<font>
<Font size="48.0"/>
</font>
</Label>
</children>
</Pane>
</top>
<center>
<GridPane BorderPane.alignment="CENTER">
<columnConstraints>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="220.0"/>
<ColumnConstraints hgrow="SOMETIMES" minWidth="1.0" prefWidth="5.0"/>
</columnConstraints>
<rowConstraints>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
</rowConstraints>
<children>
<TextField fx:id="txt1" promptText="Inserisci primo operando" GridPane.columnIndex="1"/>
<TextField fx:id="txt2" promptText="Inserisci secondo operando" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
<Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Oprando 1:">
<font>
<Font size="18.0"/>
</font>
</Label>
<Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" layoutX="10.0" layoutY="47.0" text="Oprando 2:" GridPane.rowIndex="1">
<font>
<Font size="18.0"/>
</font>
</Label>
<GridPane GridPane.rowIndex="2">
<columnConstraints>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
</columnConstraints>
<rowConstraints>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
</rowConstraints>
<children>
<Button fx:id="btnPiu" mnemonicParsing="false" onAction="#doButtonPiu" text="+"/>
<Button fx:id="btnMeno" mnemonicParsing="false" onAction="#doButtonMeno" text="-" GridPane.columnIndex="1"/>
</children>
</GridPane>
<GridPane layoutX="10.0" layoutY="160.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
<columnConstraints>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="15.0"/>
<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
</columnConstraints>
<rowConstraints>
<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
</rowConstraints>
<children>
<Button fx:id="btnDiviso" mnemonicParsing="false" onAction="#doButtonDiviso" text="/" GridPane.columnIndex="1"/>
<Button fx:id="btnPer" mnemonicParsing="false" onAction="#doButtonPer" text="*"/>
</children>
</GridPane>
<Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Risultato:" GridPane.rowIndex="3">
<font>
<Font size="18.0"/>
</font>
</Label>
<TextField fx:id="risultato" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
</children>
</GridPane>
</center>
</BorderPane>